# AI Microservice Environment Variables

# Server Configuration
HOST=0.0.0.0
PORT=8000
LOG_LEVEL=INFO

# Database Configuration (PostgreSQL - same as Java backend)
DATABASE_URL=postgresql://username:password@localhost:5432/floodguard
DB_HOST=localhost
DB_PORT=5432
DB_NAME=floodguard
DB_USER=floodguard_user
DB_PASSWORD=your_secure_password

# External APIs (Optional - for fetching additional data)
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Model Configuration
MODEL_PATH=./models
ENABLE_MODEL_CACHING=true

# Performance
MAX_WORKERS=4
REQUEST_TIMEOUT=30

# Development
DEBUG=false
RELOAD=false
